<query id="getFilesByFileSrl" action="select">
    <tables>
        <table name="files" />
    </tables>
    <conditions>
        <condition operation="in" column="file_srl" var="file_srl" notnull="notnull" />
    </conditions>
    <navigation>
        <index var="sort_index" defualt="regdate" order="order_type" />
    </navigation>
</query>
