<query id="insertElasticSearchErrorLog" action="insert">
    <tables>
        <table name="elasticsearch_error_logs" />
    </tables>
    <columns>
        <column name="act" var="act" />
        <column name="module" var="module" />
        <column name="type" var="type" maxlength="40" />
        <column name="params" var="params" />
        <column name="error" var="error" />
        <column name="member_srl" var="member_srl" default="0" />
        <column name="nick_name" var="nick_name" />
        <column name="content" var="content" />
        <column name="request_uri" var="request_uri" />
        <column name="ipaddress" notnull="notnull" var="ipaddress" />
        <column name="regdate" notnull="notnull" filter="number" var="regdate" />
    </columns>
</query>
