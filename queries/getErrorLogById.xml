<query id="getErrorLogById" action="select">
    <tables>
        <table name="elasticsearch_error_logs" />
    </tables>
    <columns>
        <column name="*" />
    </columns>
    <conditions>
        <condition operation="equal" column="error_id" var="error_id" notnull="notnull" filter="number" />
    </conditions>
</query>
